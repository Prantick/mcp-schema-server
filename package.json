{
  "name": "mcp-schema-server",
  "version": "1.0.0",
  "description": "A zero-config MCP server framework that generates a fully functional Model Context Protocol server directly from a simple manifest.json schema. Define your tools, prompts, resources, and metadata once, and mcp-schema-server instantly transforms it into a ready-to-use MCP server with no manual coding required.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "prepack": "npm run build",
    "check": "bash scripts/prepublish-check.sh",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "server",
    "framework",
    "schema-driven",
    "zero-config"
  ],
  "author": "Prantick Das",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/mcp-schema-server.git"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.22.0",
    "ajv": "^8.17.1"
  },
  "devDependencies": {
    "@types/ajv": "^0.0.5",
    "@types/node": "^24.10.1",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
